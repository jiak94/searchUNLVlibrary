{
	"manifest_version": 2,
	"name": "Search UNLV Library",
	"icons": {
		"16": "./img/ico.png",
		"32": "./img/ico.png",
		"48": "./img/ico.png",
		"128": "./img/ico.png"
	},
	"version": "0.2.2",

	
	"description": "A extension to search books from UNLV Library",

	"author": "Jiakuan Li",

	"browser_action": {
		"default_popup": "popup.html"
	},

	"permissions": [
		"https://*/*"
	]
}